{
    "manifest_version": 3,
    "name": "Go links",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": ["webRequest", "declarativeNetRequest", "storage"],
    "host_permissions": ["*://*/*"],
    "background": {
	"scripts": ["worker.js"],
        "type": "module"
    },
    "options_ui": {
	    "page": "options.html"
    },
    "browser_specific_settings": {
	    "gecko": {
		    "id": "extension@golinks"
	    }
    }
}
